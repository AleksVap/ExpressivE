{
  "dataset": "WN18RR",
  "dataset_kwargs": {
    "create_inverse_triples": false
  },
  "model": "RotatE",
  "model_kwargs": {
    "embedding_dim": 500
  },
  "loss": "NSSALoss",
  "loss_kwargs": {
    "reduction": "sum",
    "adversarial_temperature": 0.5,
    "margin": 6
  },
  "optimizer": "Adam",
  "optimizer_kwargs": {
    "lr": 1e-03
  },
  "training_loop": "SLCWATrainingLoop",
  "negative_sampler": "BasicNegativeSampler",
  "negative_sampler_kwargs": {
    "num_negs_per_pos": 1024
  },
  "training_kwargs": {
    "num_epochs": 1000,
    "checkpoint_frequency": 60,
    "checkpoint_name": "checkpoint.pt",
    "checkpoint_on_failure": true,
    "batch_size": 512
  },
  "stopper": "early",
  "stopper_kwargs": {
    "patience": 10,
    "relative_delta": 0.005,
    "frequency": 10
  },
  "evaluator": "RankBasedEvaluator",
  "evaluator_kwargs": {
    "filtered": true,
    "batch_size": 1
  },
  "device": "gpu"
}